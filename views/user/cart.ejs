                                                           value="<%= item.quantity %>" 
                                                           min="1" 
                                                           max="<%= Math.min(5, item.productId.stockQuantity) %>"
                                                           data-stock="<%= item.productId.stockQuantity %>"
                                                           onchange="checkQuantity('<%= item.productId._id %>', this.value, <%= item.productId.stockQuantity %>)">
                                                </td>
                                                
                                                <td class="text-center align-middle item-total">
                                                    ₹<%= (item.discountedPrice * item.quantity).toFixed(2) %>
                                                </td>
                                                <td class="text-center align-middle">
                                                    <button onclick="removeItem('<%= item.productId._id %>')" class="btn btn-danger btn-sm">Remove</button>
