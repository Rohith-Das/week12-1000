<script>
    async function updateQuantity(productId, quantity) {
      try {
        quantity = parseInt(quantity);
        if (isNaN(quantity) || quantity < 1) {
          Swal.fire({
            icon: 'error',
            title: 'Invalid Quantity',
            text: 'Please enter a valid quantity.',
          });
          resetQuantityInput(productId);
          return;
        }
    
        const response = await fetch('/update-cart', {
          method: 'POST',
          headers: { 'Content-Type': 'application/json' },
          body: JSON.stringify({ productId, quantity })
        });
        
        const data = await response.json();
    
        if (data.success) {
          const itemRow = document.querySelector(`tr[data-product-id="${productId}"]`);
          const itemTotalCell = itemRow.querySelector('.item-total');
          const priceCell = itemRow.querySelector('td:nth-child(3)');
          const discountedPrice = parseFloat(priceCell.querySelector('.discounted-price')?.textContent.replace('₹', '') || priceCell.textContent.replace('₹', ''));
    
          itemTotalCell.textContent = `₹${(discountedPrice * quantity).toFixed(2)}`;
          updateCartTotals();
        } else {
          // Handle different error cases
          if (data.error.includes('Maximum quantity limit is 5')) {
            Swal.fire({
              icon: 'error',
              title: 'Quantity Limit Exceeded',
              text: 'You cannot add more than 5 of this item to your cart.',
            });
            resetQuantityInput(productId, 5);
          } else if (data.error.includes('available in stock')) {
            Swal.fire({
              icon: 'warning',
              title: 'Stock Limit Exceeded',
              text: data.error,
            });
            resetQuantityInput(productId, data.stockQuantity);
          } else {
            Swal.fire({
              icon: 'error',
              title: 'Error',
              text: data.error || 'Failed to update cart',
            });
            resetQuantityInput(productId);
          }
        }
      } catch (error) {
        console.error('Error:', error);
        Swal.fire({
          icon: 'error',
          title: 'Error',
          text: 'Failed to update cart',
        });
        resetQuantityInput(productId);
      }
    }
    
    function resetQuantityInput(productId, value) {
      const inputField = document.querySelector(`tr[data-product-id="${productId}"] input[type="number"]`);
      if (inputField) {
        if (value !== undefined) {
          inputField.value = value;
        } else {
          // Reset to previous valid value
          const currentValue = parseInt(inputField.getAttribute('data-last-valid') || 1);
          inputField.value = currentValue;
        }
      }
    }
    
    function updateCartTotals() {
        let total = 0; 
        const itemRows = document.querySelectorAll('#cart-items tr');
    
        itemRows.forEach(itemRow => {
            const quantity = Number(itemRow.querySelector('input[type="number"]').value);
            const priceCell = itemRow.querySelector('td:nth-child(3)');
            const discountedPrice = parseFloat(priceCell.querySelector('.discounted-price')?.textContent.replace('₹', '') || priceCell.textContent.replace('₹', ''));
    
            total += discountedPrice * quantity;
        });
    
        document.getElementById('summary-total').textContent = total.toFixed(2);
        const itemCount = itemRows.length;
        document.getElementById('item-count').textContent = itemCount;
        const checkoutButton = document.getElementById('checkout-button');
        if (itemCount === 0) {
            document.querySelector('.card-body p').textContent = 'Your cart is empty.';
            checkoutButton.disabled = true;
        } else {
            checkoutButton.disabled = false;
        }
    }
    
    async function removeItem(productId) {
        try {
            const response = await fetch('/remove-from-cart', {
                method: 'POST',
                headers: { 'Content-Type': 'application/json' },
                body: JSON.stringify({ productId })
            });
            const data = await response.json();
    
            if (data.success) {
                // Remove the item row from the cart
                const itemRow = document.querySelector(`tr[data-product-id="${productId}"]`);
                itemRow.remove();
    
                // Update the cart totals
                updateCartTotals();
            } else {
                console.error('Failed to remove item:', data.error);
            }
        } catch (error) {
            console.error('Error:', error);
        }
    }
    
    // Initialize event listeners when DOM is loaded
    document.addEventListener('DOMContentLoaded', function() {
        // Add event listeners for quantity inputs
        document.querySelectorAll('#cart-items input[type="number"]').forEach(input => {
            // Store initial value
            input.setAttribute('data-last-valid', input.value);
            
            input.addEventListener('change', function() {
                const productId = this.closest('tr').dataset.productId;
                const newValue = parseInt(this.value);
                
                if (newValue <= 0) {
                    this.value = this.getAttribute('data-last-valid') || 1;
                    Swal.fire({
                        icon: 'error',
                        title: 'Invalid Quantity',
                        text: 'Quantity must be at least 1',
                    });
                    return;
                }
                
                updateQuantity(productId, newValue);
            });
            
            // Store last valid value before change
            input.addEventListener('focus', function() {
                this.setAttribute('data-last-valid', this.value);
            });
        });
    
        // Add event listeners for remove buttons
        document.querySelectorAll('#cart-items button.btn-danger').forEach(button => {
            button.addEventListener('click', function() {
                const productId = this.closest('tr').dataset.productId;
                removeItem(productId);
            });
        });
    });
    </script>

	                		
