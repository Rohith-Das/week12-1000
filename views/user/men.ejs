                                /* Card and Product Styles */
/* Card and Product Styles */
.product {
    /* border: 1px solid #ddd; */
    border-radius: 8px;
    overflow: hidden;
    box-shadow: 0 2px 5px rgba(0,0,0,0.1);
    transition: transform 0.3s, box-shadow 0.3s;
    display: flex;
    flex-direction: column;
}

.product:hover {
    transform: translateY(-5px);
    box-shadow: 0 4px 10px rgba(0,0,0,0.2);
}

.product-media {
    position: relative;
}

.product-label {
    position: absolute;
    top: 10px;
    left: 10px;
    padding: 5px 10px;
    background: rgba(0, 0, 0, 0.7);
    color: #fff;
    border-radius: 3px;
    font-size: 12px;
}

.product-image {
    width: 100%;
    height: auto;
    transition: opacity 0.3s;
}

.product-image:hover {
    opacity: 0.8;
}

/* Button Styles */
.card-footer {
    padding: 0.75rem 1.25rem;
    /* border-top: 1px solid #ddd; */
    background: #f8f9fa;
}

.btn-product {
    display: block;
    width: 100%;
    background-color: #007bff;
    color: #fff;
    border: none;
    padding: 10px 0;
    border-radius: 4px;
    font-size: 14px;
    text-transform: uppercase;
    transition: background-color 0.3s, transform 0.2s;
    cursor: pointer;
}

.btn-product:hover {
    background-color: #0056b3;
    transform: translateY(-2px);
}

.btn-product span {
    font-weight: bold;
}

.btn-product-icon {
    background: rgba(0,0,0,0.2);
    color: #fff;
    border-radius: 50%;
    width: 40px;
    height: 40px;
    display: flex;
    align-items: center;
    justify-content: center;
    transition: background-color 0.3s;
}

.btn-product-icon:hover {
    background: #007bff;
}

.product-cat a {
    color: #007bff;
    text-decoration: none;
}

.product-cat a:hover {
    text-decoration: underline;
}

.product-title a {
    color: #333;
    text-decoration: none;
    font-weight: bold;
}

.product-title a:hover {
    color: #007bff;
    text-decoration: underline;
}

.product-price {
    font-size: 16px;
    font-weight: bold;
}
.label-out-of-stock {
    background-color: #ff0000;
    color: #ffffff;
    padding: 5px 10px;
    position: absolute;
    top: 10px;
    right: 10px;
    z-index: 1;
}

.btn-cart.disabled {
    opacity: 0.5;
    cursor: not-allowed;
}
.wishlist-icon {
        top: 10px;
        right: 10px;
        font-size: 24px;
        color: #d1eb72;
        transition: color 0.3s;
    }
    .wishlist-icon:hover {
        color: #e74c3c;
    }
    .position-relative {
        position: relative;
    }
    .position-absolute {
        position: absolute;
    }

                            </style>
                            
                            <script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/js/all.min.js"></script>
            
                          <div class="products mb-3">
    <div class="row justify-content-center" id="product-container">
        <% if (products && products.length) { %>
            <% products.forEach(product => { %>
                <div class="col-lg-3 col-md-4 col-sm-6 mb-4 product-item">
                    <div class="product product-7 text-center card">
                        <figure class="product-media">
                            <% if (product.discountPercentage > 0) { %>
                                <span class="product-label label-discount">
                                    <%= product.discountPercentage %>% Off
                                </span>
                            <% } %>
                            <% if (typeof product.stockQuantity === 'number' && product.stockQuantity === 0) { %>
                                <span class="product-label label-out-of-stock">Out of Stock</span>
                            <% } %>

                            <a href="/singleProduct/<%= product._id %>">
                                <img src="<%= product.images[0] %>" class="card-img-top product-image" alt="<%= product.productName %>">
                            </a>
                        </figure>
                        <div class="card-body">
                            <h5 class="product-name"><%= product.productName %></h5>
                            <% if (product.discountedPrice < product.price) { %>
                                <p class="product-price">
                                    <span class="original-price" style="text-decoration: line-through; color: #999;">
                                        ₹<%= product.price.toLocaleString('en-IN', { style: 'currency', currency: 'INR' }).slice(1) %>
                                    </span>
                                    <span class="discounted-price" style="color: #e74c3c; font-weight: bold;">
                                        ₹<%= product.discountedPrice.toLocaleString('en-IN', { style: 'currency', currency: 'INR' }).slice(1) %>
                                    </span>
                                </p>
                            <% } else { %>
                                <p class="product-price">
                                    ₹<%= product.price.toLocaleString('en-IN', { style: 'currency', currency: 'INR' }).slice(1) %>
                                </p>
                            <% } %>
                        </div>
                        <div class="card-footer">
                            <% if (product.stockQuantity > 0) { %>
                                <form action="/add-to-cart/<%= product._id %>" method="POST">
                                    <button type="submit" class="btn-product btn-cart">
                                        <span>Add to Cart</span>
                                    </button>
                                </form>
                            <% } else { %>
                                <button class="btn-product btn-cart disabled" disabled>
                                    <span>Out of Stock</span>
                                </button>
                            <% } %>
                        </div>
                    </div>
                </div>
            <% }); %>
        <% } else { %>
            <p>No products found.</p>
        <% } %>
    </div>
    <div class="products mb-3">
        <div class="row justify-content-center" id="product-container">
            <% if (products && products.length) { %>
                <% products.forEach(product => { %>
                    <!-- Product cards here -->
                <% }); %>
            <% } else { %>
                <p>No products found.</p>
            <% } %>
        </div>
    </div>
    
    <!-- Pagination -->
    <nav aria-label="Page navigation example">
      <ul class="pagination justify-content-center">
        <% if (currentPage > 1) { %>
          <li class="page-item">
            <a class="page-link" href="?page=<%= currentPage - 1 %>&limit=<%= limit %>">Previous</a>
          </li>
        <% } %>
        
        <% for (let i = 1; i <= totalPages; i++) { %>
          <li class="page-item <%= currentPage === i ? 'active' : '' %>">
            <a class="page-link" href="?page=<%= i %>&limit=<%= limit %>"><%= i %></a>
          </li>
        <% } %>
        
        <% if (currentPage < totalPages) { %>
          <li class="page-item">
            <a class="page-link" href="?page=<%= currentPage + 1 %>&limit=<%= limit %>">Next</a>
          </li>
        <% } %>
      </ul>
    </nav>
    
