<!-- Content wrapper -->
<div class="content-wrapper">
    <h1>Coupon Management</h1>
    <!-- Content -->
    <div class="container-xxl flex-grow-1 container-p-y">
      <div class="container-fluid main-content">
        <div class="d-flex justify-content-between align-items-center mb-4">
          <button onclick="window.location.href='/admin/dashboard/coupons/create'">Create Coupon</button>
        </div>
      </div>
    </div>
  </div>

<!-- Coupon Table -->
<div class="row mb-4">
  <div class="card shadow">
    <div class="card-body table-responsive">
      <table class="table table-striped table-hover align-middle">
        <thead class="table-dark">
          <tr>
            <th>Coupon Code</th>
            <th>Description</th>
            <th>Discount (%)</th>
            <th>Min Amount</th>
            <th>Max Discount</th>
            <th>Expiry Date</th>
            <th>Status</th>
            <th class="text-center">Actions</th>
          </tr>
        </thead>
        <tbody>
          <% if (coupons && coupons.length > 0) { %>
            <% coupons.forEach(function(coupon) { %>
              <tr>
                <td><%= coupon.code %></td>
                <td><%= coupon.description %></td>
                <td><%= coupon.discount %></td>
                <td><%= coupon.minAmount %></td>
                <td><%= coupon.maxDiscount %></td>
                <td><%= coupon.expiryDate.toDateString() %></td>
                <td>
                  <span class="badge <%= coupon.status === 'active' ? 'bg-success' : 'bg-secondary' %>">
                    <%= coupon.status === 'active' ? 'Listed' : 'Unlisted' %>
                  </span>
                </td>
                <td class="text-center">
                  <button class="btn btn-sm btn-warning me-2" onclick="openEditModal('<%= coupon._id %>')">
                    <i class="fas fa-edit"></i> Edit
                  </button>
                 
                  <form action="/admin/dashboard/coupons/toggle-status/<%= coupon._id %>" method="POST" style="display:inline;" id="toggleForm-<%= coupon._id %>">
                    <button type="button" class="btn btn-sm <%= coupon.status === 'active' ? 'btn-danger' : 'btn-success' %>" onclick="confirmToggleStatus('<%= coupon._id %>', '<%= coupon.status === 'active' ? 'unlist' : 'list' %>')">
                      <i class="fas <%= coupon.status === 'active' ? 'fa-times-circle' : 'fa-check-circle' %>"></i>
                      <%= coupon.status === 'active' ? 'Unlist' : 'List' %>
                    </button>
                  </form>
                </td>
              </tr>
            <% }); %>
          <% } else { %>
            <tr>
              <td colspan="8" class="text-center">No coupons found</td>
            </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </div>
</div>

<nav aria-label="Page navigation">
  <ul class="pagination justify-content-center">
    <% if (hasPrevPage) { %>
      <li class="page-item">
        <a class="page-link" href="/admin/dashboard/coupons?page=<%= currentPage - 1 %>" aria-label="Previous">
          <span aria-hidden="true">&laquo;</span>
        </a>
      </li>
    <% } %>
    
    <% for (let i = 1; i <= totalPages; i++) { %>
      <li class="page-item <%= i === currentPage ? 'active' : '' %>">
        <a class="page-link" href="/admin/dashboard/coupons?page=<%= i %>"><%= i %></a>
      </li>
    <% } %>
    
    <% if (hasNextPage) { %>
      <li class="page-item">
        <a class="page-link" href="/admin/dashboard/coupons?page=<%= currentPage + 1 %>" aria-label="Next">
          <span aria-hidden="true">&raquo;</span>
        </a>
      </li>
    <% } %>
  </ul>
</nav>
<p>
  <!-- <%= totalCoupons %> -->


<div id="editCouponModal" class="modal fade" tabindex="-1" aria-labelledby="editCouponModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="editCouponModalLabel">Edit Coupon</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <form id="editCouponForm" action="/admin/dashboard/coupons/edit" method="POST" onsubmit="return validateForm()">
          <input type="hidden" id="editCouponId" name="id">
          
          <div class="mb-3">
            <label for="editCode" class="form-label">Coupon Code:</label>
            <input type="text" id="editCode" name="code" class="form-control" required minlength="3" maxlength="20">
