<script>
  function validateForm() {
    let isValid = true;

    // Validate Coupon Code
    const codeInput = document.getElementById('editCode');
    if (codeInput.value.trim().length < 3 || codeInput.value.trim().length > 20) {
      codeInput.classList.add('is-invalid');
      isValid = false;
    } else {
      codeInput.classList.remove('is-invalid');
    }

    // Validate Description (optional, but check length if provided)
    const descriptionInput = document.getElementById('editDescription');
    if (descriptionInput.value.trim().length > 100) {
      descriptionInput.classList.add('is-invalid');
      isValid = false;
    } else {
      descriptionInput.classList.remove('is-invalid');
    }

    // Validate Discount Percentage
    const discountInput = document.getElementById('editDiscount');
    if (discountInput.value < 0 || discountInput.value > 50) {
      discountInput.classList.add('is-invalid');
      isValid = false;
    } else {
      discountInput.classList.remove('is-invalid');
    }

    // Validate Minimum Amount
    const minAmountInput = document.getElementById('editMinAmount');
    if (minAmountInput.value < 0) {
      minAmountInput.classList.add('is-invalid');
      isValid = false;
    } else {
      minAmountInput.classList.remove('is-invalid');
    }

    // Validate Maximum Discount (optional, but must be positive if provided)
    const maxDiscountInput = document.getElementById('editMaxDiscount');
    if (maxDiscountInput.value && maxDiscountInput.value < 0) {
      maxDiscountInput.classList.add('is-invalid');
      isValid = false;
    } else {
      maxDiscountInput.classList.remove('is-invalid');
    }

    // Validate Expiry Date
    const expiryDateInput = document.getElementById('editExpiryDate');
    if (!expiryDateInput.value) {
      expiryDateInput.classList.add('is-invalid');
      isValid = false;
    } else {
      expiryDateInput.classList.remove('is-invalid');
    }

    return isValid;
  }





  // SweetAlert Confirmation for List/Unlist Action
  function confirmToggleStatus(couponId, action) {
    Swal.fire({
      title: `Are you sure you want to ${action} this coupon?`,
      icon: 'warning',
      showCancelButton: true,
      confirmButtonColor: '#3085d6',
      cancelButtonColor: '#d33',
      confirmButtonText: `Yes, ${action} it!`
    }).then((result) => {
      if (result.isConfirmed) {
        // Submit the form instead of redirecting
        document.getElementById(`toggleForm-${couponId}`).submit();F
      }
    });
  }

  function openEditModal(couponId) {
    // Fetch coupon details and populate the form
    fetch(`/admin/dashboard/coupons/${couponId}`)
      .then(response => response.json())
      .then(coupon => {
        document.getElementById('editCouponId').value = coupon._id;
        document.getElementById('editCode').value = coupon.code;
        document.getElementById('editDescription').value = coupon.description;
        document.getElementById('editDiscount').value = coupon.discount;
        document.getElementById('editMinAmount').value = coupon.minAmount;
        document.getElementById('editMaxDiscount').value = coupon.maxDiscount;
        document.getElementById('editExpiryDate').value = new Date(coupon.expiryDate).toISOString().split('T')[0];
        new bootstrap.Modal(document.getElementById('editCouponModal')).show();
      });
  }
  
  function closeEditModal() {
    bootstrap.Modal.getInstance(document.getElementById('editCouponModal')).hide();
  }
</script>



